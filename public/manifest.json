{
  "manifest_version": 2,
  "name": "JW Dash-Lite",
  "author": "Daniel Lee",
  "version": "1.0.0",
  "description": "Access some JW Dashboard features through a Chrome extension.",
  "icons": {
    "16": "./preview_imgs/16.png",
    "48": "./preview_imgs/48.png",
    "128": "./preview_imgs/128.png"
  },
  "browser_action": {
    "default_icon": {          
      "16": "./preview_imgs/16.png", 
      "24": "./preview_imgs/24.png", 
      "32": "./preview_imgs/32.png"  
    },
    "default_title": "Dash-Lite",      
    "default_popup": "index.html"       
  }
}